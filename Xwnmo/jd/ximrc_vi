setuumrc lt_LN /usr/local/lib/wnn/lt_LN/uumrc.xim
setuumrc ko_KR /usr/local/lib/wnn/ko_KR/uumrc.xim
preloadrkfile ja_JP
;preloadrkfile zh_CN
;preloadrkfile de_DE
