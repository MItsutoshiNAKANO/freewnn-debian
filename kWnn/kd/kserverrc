jserver_dir	@LIBDIR/@LANG/dic
readfile	sys/hword.dic
readfile	sys/single.dic

readfile	sys/full.fzk

;         n nbun hindo len jiri flag jisho sbn dbn_len sbn_cnt suji kana eisuu kigou toji fuzoku kai
def_param 2 10   2     45  5    100  5     1   15      0       400  0    400   80    200  2      200
