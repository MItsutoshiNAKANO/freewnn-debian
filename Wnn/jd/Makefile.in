install::  $(SERVERCDS)
	@if [ -d $(DESTDIR)$(JWNNWNNDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(JWNNWNNDIR)); fi
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[i]*) set +e;; esac; done; \
	for i in  $(SERVERCDS); do \
	(set -x; $(INSTALL) $(INSTDATFLAGS) $$i $(DESTDIR)$(JWNNWNNDIR)); \
	done

