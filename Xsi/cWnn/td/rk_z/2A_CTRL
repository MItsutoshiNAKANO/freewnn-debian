(defvar pf1 (list '\x81') )
(defvar pf2 (list '\x82') )
(defvar pf3 (list '\x83') )
(defvar pf5 (list '\x85'))

(unless ZY)(pf1)	(on ZY)(off ASCII)(off GONG_NENG)
(if ZY)(if ZHUYIN)(pf1)	(off ZHUYIN)(on ZHUYIN)

(unless ASCII)(pf3)		(on ASCII)(off ZY)(off GONG_NENG)
(if ASCII)(if ban_jiao)(pf3)	(off ban_jiao)(on quan_jiao)
(if ASCII)(if quan_jiao)(pf3)	(off quan_jiao)(on ban_jiao)

(pf5)   (switch GONG_NENG)      H
