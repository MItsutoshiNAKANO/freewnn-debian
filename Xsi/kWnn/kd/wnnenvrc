confirm1
setdic sys/hword.dic	usr/@USR/hword.h	5 1 0 - - 0
setdic sys/single.dic	usr/@USR/single.h	1 1 0 - - 0

setdic usr/@USR/ud	-			2 0 0 - - 0

setfuzokugo sys/full.fzk

katakana_gakusyuu       temporary                         0
;katakana_gakusyuu      usr/@USR/hangul		10        0
;katakana_gakusyuu      off

bunsetsugiri_gakusyuu   temporary                         0
;bunsetsugiri_gakusyuu  usr/@USR/fuzokugo       10        0
;bunsetsugiri_gakusyuu  off


;;       N nsho hindo len jiri flag jisho sbn dbn_len sbn_cnt suuji kana eisuu kigou toji_kakko fuzokogo kaikakko
setparam 2 5    2     45  200  80   5     1   40      0       400   -100 400   80    200        2        200
