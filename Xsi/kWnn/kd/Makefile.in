include @top_srcdir@/makerule.mk

INSTALL      = @INSTALL@
INSTALLFLAGS = $(INSTINCFLAGS)

SUBDIRS = rk

SERVERKDS = kserverrc hinsi.data

CLIENTKDS = uum.msg 	uumkey 	uumrc 	wnnstat.msg

