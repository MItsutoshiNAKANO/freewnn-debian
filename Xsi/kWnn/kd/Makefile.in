LIBKDS2 = serverdefs

LINKS = $(CLIENTSKDS2) $(LIBKDS2)

all:: $(LINKS)

includes:: $(LINKS)

cvt_key_empty:  $(WNNCNVFILEDIR)/cvt_key_empty
