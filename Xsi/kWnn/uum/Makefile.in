install:: kuum
	@if [ -d $(DESTDIR)$(KWNNBINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(KWNNBINDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS) $(LOCAL_INSTFLAGS) kuum $(DESTDIR)$(KWNNBINDIR)/kuum

$(OBJ2) : $(SRC2)
	$(CC) -c $(CFLAGS)  $(KWNNETCSRC)/`basename $@ .o`.c

$(OBJ3) : $(SRC3)
	$(CC) -c $(CFLAGS)  $(KWNNETCSRC)/`basename $@ .o`.c

$(OBJ4) : $(SRC4)
	$(CC) -c $(CFLAGS)  $(KWNNETCSRC)/`basename $@ .o`.c

$(OBJ5) : $(SRC5)
	$(CC) -c $(CFLAGS)  $(KWNNETCSRC)/`basename $@ .o`.c

depend:: .depend


clean::
	$(RM) $(HDRS) $(SRC1)
